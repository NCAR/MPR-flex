&INIT_CONTROL
filelist_name            = "FILELIST"
cellfrac_name            = "CELLFRAC"
origparam_name           = "ORIGPARAM"
calibparam_name          = "CALIPARAM"
region_info              = "INFO"
sim_dir                  = "OUTDIR"
obs_name                 = "CALIBFLOW"
executable               = "EXE"
basin_objfun_weight_file = "OBJWEIGHT"
calpar                   = "calBeta.txt"
idModel                  = "IDMODEL"

eval_length = "daily"

dt            = 86400
sim_len       = 6940
start_cal     = 3652
end_cal       = 6940
opt           = 1      !opt = 1 runs optimization code
upscale_flag  = 0      !flag to run Naoki's upscaling code

nbasin    = 1 
nHru      = 1          ! Sum of hrus consisting of each basin

Npro     = 1           ! Number of processors to use
initcell = 1           ! index of the first cell assigned to each processor
endcell  = 1           ! index of the last cell assigned to each processor
/

!---------------------------------------
! DDS parameters
!---------------------------------------
&DDS
NparCal       =  6                            ! number of parameters listed in namelist
r             =  0.2                           ! perturbation window (default 0.2)
isRestart     =  T                             ! F -> start with the begining, T -> start with restart file
seed          =  32                            ! starting seed for random number generator
maxiter       =  1000                          ! maximum number of objective function evaluation (i.e., iteration) 
maxit         =  F                             ! maximization (.True.) or minimization (.False.) 
restrt_file   = "./restart_dds.txt"
state_file    = "./region_xx_sceua_output.txt"
/
